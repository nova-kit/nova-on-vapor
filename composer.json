{
    "name": "nova-kit/nova-on-vapor",
    "description": "Nova on Vapor Integration Library",
    "type": "library",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NovaKit\\NovaOnVapor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NovaKit\\NovaOnVapor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "laravel/nova": "^4.14",
        "laravel/vapor-core": "^2.22",
        "nova-kit/nova-queued-export-as-csv": "^0.1 || ^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.25 || ^7.13",
        "orchestra/canvas": "^6.10.3 || ^7.2.1",
        "nunomaduro/larastan": "^1.0.1 || ^2.0",
        "league/flysystem-aws-s3-v3": "^1.0 || ^3.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "NovaKit\\NovaOnVapor\\LaravelServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "ci": [
            "@composer run prepare",
            "@php vendor/bin/phpstan analyse",
            "@php vendor/bin/pint",
            "@composer run test"
        ],
        "test": "@php vendor/bin/phpunit -c ./ --color"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
